<%- include('./../_partials/head') %>


<h1>Users</h1>


<% users.forEach(function(user) { %>

    <div class="user">
        <h3>Name: <%= user.name %></h3>
        <h4>Age: <%= user.age %></h4>
        <a href="/users/<%= user._id %>/posts">See all my posts</a>
    </div>

<% }) %>


